<script>
    const sidebar = document.getElementById("default-sidebar");
    const toggleBtn = document.getElementById("sidebar-toggle");
    const closeBtn = document.getElementById("sidebar-close");

    toggleBtn.addEventListener("click", () => {
        sidebar.classList.remove("-translate-x-full");
    });

    closeBtn.addEventListener("click", () => {
        sidebar.classList.add("-translate-x-full");
    });
</script>
<script>
  const btn = document.getElementById('notif-btn');
  const menu = document.getElementById('notif-menu');

  btn.addEventListener('click', () => {
    menu.classList.toggle('hidden');
  });

  // Optional: Close when clicking outside
  window.addEventListener('click', function (e) {
    if (!btn.contains(e.target) && !menu.contains(e.target)) {
      menu.classList.add('hidden');
    }
  });
</script>
<script>
  const rolebtn = document.getElementById('roleDropdownBtn');
  const rolemenu = document.getElementById('roleDropdown');

  rolebtn.addEventListener('click', () => {
    rolemenu.classList.toggle('hidden');
  });

  // Optional: Close when clicking outside
  window.addEventListener('click', function (e) {
    if (!rolebtn.contains(e.target) && !rolemenu.contains(e.target)) {
      rolemenu.classList.add('hidden');
    }
  });
</script>
<script>
  // Sare buttons aur content ko select karenge
  const tabButtons = document.querySelectorAll(".vd-filter-btn");
  const tabContents = document.querySelectorAll(".vd-user-row");

  tabButtons.forEach((btn) => {
    btn.addEventListener("click", () => {
      const target = btn.getAttribute("data-role");

      // Sabhi buttons se active class hata do
      tabButtons.forEach((b) =>
        b.classList.remove("vd-user-row-active")
      );
      // Sirf clicked button ko active banao
      btn.classList.add("vd-user-row-active");

      // Sabhi content hide karo
      tabContents.forEach((content) => {
        content.classList.remove("vd-user-row-active");
        content.style.display = "none";
      });

      // Target content show karo
      const activeContent = document.querySelector(
        `.vd-user-row[data-role="${target}"]`
      );
      if (activeContent) {
        activeContent.classList.add("vd-user-row-active");
        activeContent.style.display = "grid"; // grid kyunki aapne grid lagayi hai
      }
    });
  });

  // Default me pehle tab ko active rakhenge
  document.addEventListener("DOMContentLoaded", () => {
    tabButtons[0].click();
  });
</script>
